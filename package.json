{
  "name": "impulse",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.0",
    "emotion": "^9.2.10",
    "global": "^4.3.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.10",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-most": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/runtime": "7.0.0",
    "@svgr/webpack": "2.4.1",
    "autoprefixer": "9.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.2",
    "babel-plugin-emotion": "^9.2.10",
    "babel-plugin-named-asset-import": "1.0.0-next.2150693d",
    "babel-preset-react-app": "4.0.0-next.2150693d",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "0.28.11",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "3.0.0-next.2150693d",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "1.1.11",
    "fs-extra": "7.0.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "loader-utils": "1.1.0",
    "mini-css-extract-plugin": "0.4.3",
    "node-sass": "^4.9.3",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.14.3",
    "promise": "8.0.2",
    "raf": "3.4.0",
    "react-dev-utils": "6.0.0-next.2150693d",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "terser-webpack-plugin": "1.1.0",
    "thread-loader": "1.2.0",
    "url-loader": "1.1.1",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-server": "3.1.8",
    "webpack-manifest-plugin": "2.0.4",
    "webpackbar": "^2.6.3",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(graphql)$": "<rootDir>/config/jest/graphqlTransform.js",
      "^(?!.*\\.(js|jsx|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "emotion"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.25%",
    "not op_mini all",
    "ie 11"
  ]
}
